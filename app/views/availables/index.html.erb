
<body>
<center>
<h1> Jobs availables for you!</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.title %></td>
        <td><%= job.description %></td>
        <td><%= job.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<center>
</body>
